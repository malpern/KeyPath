<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Bundle identifier (must match LaunchDaemon Label and file name) -->
    <key>CFBundleIdentifier</key>
    <string>com.keypath.helper</string>

    <!-- Helper name -->
    <key>CFBundleName</key>
    <string>KeyPathHelper</string>

    <!-- Version (should match main app) -->
    <key>CFBundleShortVersionString</key>
    <string>1.1.0</string>

    <key>CFBundleVersion</key>
    <string>2</string>

    <!-- Info dictionary version -->
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>

    <!-- SMAppService path: this helper is packaged inside the app bundle and registered by the host app. No SMAuthorizedClients needed. -->
</dict>
</plist>
